{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.svg",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/health",
      "dest": "app.py"
    },
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/config.js",
      "dest": "/config.js"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/theme.js",
      "dest": "/theme.js"
    },
    {
      "src": "/recent.js",
      "dest": "/recent.js"
    },
    {
      "src": "/settings.js",
      "dest": "/settings.js"
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "/recent.html",
      "dest": "/recent.html"
    },
    {
      "src": "/settings.html",
      "dest": "/settings.html"
    },
    {
      "src": "/",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}